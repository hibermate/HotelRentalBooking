@model HotelRentalBookingClient2.DataFormsModel.OccupacyModel
@{
    ViewBag.Title = "AddOccupacyPage";
}

<div class="form-horizontal form-label-left">
    <section class="login_content">

        <h1>Login Form</h1>
        @using (Html.BeginForm("Occupacy", "AddNew", FormMethod.Post))
        {
            @Html.AntiForgeryToken()


            <form>
                <fieldset>
                  @Html.TextBoxFor(model => model.OccupacyNumber, new { @class = "form-control", @autofocus = "autofocus", placeholder = "Tên đăng nhập" })

                </fieldset>

            </form>
        }

        <div class="clearfix"></div>
        <div class="separator">
            <div class="clearfix"></div>
            <br />
            <div>
                <h1><i class="fa fa-paw"></i> Gentelella Alela!</h1>
                <p>©2016 All Rights Reserved. Gentelella Alela! is a Bootstrap 3 template. Privacy and Terms</p>
            </div>
        </div>

    </section>



</div>